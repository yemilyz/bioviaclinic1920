														<sci:value>if ( @&apos;Read Conformations&apos; and @NumberOfConformations is defined and @NumberOfConformations ) then
	for #i in 1 .. @NumberOfConformations loop
			if ( FileSize(@TempMol2) &gt; 0 ) then
				@MOL2FileList .= format(&apos;%s.%d, &apos;,@TempMol2,#i);
			else
				DebugMessageError(&quot;File is missing: &quot;.format(&apos;%s.%d&apos;,@TempMol2,#i));
			end if;	
	end loop;
	@MOL2FileList := rsubst(@MOL2FileList,&apos;,\s*$&apos;,&apos;&apos;);
else
	if ( FileSize(@TempMol2) &gt; 0 ) then
		@MOL2FileList := @TempMol2;	
	else
		DebugMessageError(&quot;File is missing: &quot;.@MOL2FileList);
	end if;	
end if;</sci:value>
