												<sci:value>if ( @SourceTag is defined and ToUpper(@SourceTag)  ne &apos;NONE&apos;) then
	#FileNameInput := @OutPutFile;
	#FilenameAndExtension := rsubst(#FileNameInput, &apos;^.*[\/\\]&apos;, &apos;&apos;);
	#FileName := rsubst(#FilenameAndExtension, &apos;\.\w+$&apos;, &apos;&apos;);	
	#status := True;
	#str := #FileName;

	if ( @&apos;Read Conformations&apos; and property(&apos;NumberOfConformations&apos;) is defined ) then		
		#NumberOfConformations := property(&apos;NumberOfConformations&apos;);
		if ( #ConformationCounter == #NumberOfConformations ) then
				#ConformationCounter := 0;
		end if;
		
		#ConformationCounter++;
		if ( #ConformationCounter == 1 ) then
			#FileCounter++;
		end if;	
	
	else
		#ConformationCounter := 0;
		#FileCounter++;
	end if;
	
	if ( @SourceTag eq &apos;Filename&apos; ) then
		#str := #FileName;
	elsif ( @SourceTag eq &apos;FilenameAndExtension&apos; ) then
		#str := #FilenameAndExtension;
	elsif ( @SourceTag eq &apos;FullFilename&apos; ) then
		#str := @InitialSourceFile;
	elsif ( @SourceTag eq &apos;Number&apos; ) then
		#str := #FileCounter;		
	elsif ( @SourceTag eq &apos;Letter&apos; ) then
		#str := chr(#FileCounter+64);
	elsif ( @SourceTag eq &apos;None&apos; ) then
		remove (&apos;SourceTag&apos;);
		#status := False;
	elsif ( @SourceTag eq &apos;KeepCurrent&apos; ) then
		#str := @InitialSourceFile;
	else			
		#status := False;	
	end if;	
	
	if ( #status ) then
		property(&apos;SourceTag&apos;) := #str;
	end if;	
end if;</sci:value>
