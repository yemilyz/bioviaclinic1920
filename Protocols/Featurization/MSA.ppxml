						<sci:component name="Reassemble Aligned Sequences" version="2">
							<sci:arg name="Merge Using" type="StringType" required="false" visibility="visible" multi="false" expanded="true" uimpl="true">
								<sci:metaproplist>
									<sci:mpropval name="SCI_PromotedFrom" ishidden="true" type="StringType">
