							</sci:arg>
							<sci:arg name="If Multiple Hits" type="StringType" required="false" visibility="visible" multi="false" uimpl="true">
								<sci:metaproplist>
									<sci:mpropval name="SCI_PromotedFrom" ishidden="true" type="StringType">
										<sci:value>If Multiple Hits</sci:value>
