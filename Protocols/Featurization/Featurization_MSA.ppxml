@gzString := &apos;.gz&apos;;
@zipString := &apos;.zip&apos;;

@fileNameLength := stringlen(@Source);

@lastThreeChars := substring(@Source, @fileNameLength - 2, 3);
@lastFourChars := substring(@Source, @fileNameLength - 3, 4);

@isCompressed := (@lastThreeChars eq @gzString) or (@lastFourChars eq @zipString)</sci:value>
											</sci:arg>
